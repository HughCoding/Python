{
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.editor.empty.hint": "hidden",
    "hediet.vscode-drawio.resizeImages": null,
    "workbench.iconTheme": "ayu",
    "accessibility.signals.terminalBell": {
        "sound": "on"
    },
    "terminal.integrated.enableVisualBell": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "liveServer.settings.donotVerifyTags": true,
    "diffEditor.hideUnchangedRegions.enabled": true,
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "notebook.lineNumbers": "on",
    "editor.minimap.enabled": false,
    "workbench.colorTheme": "Ayu Dark",
    "tabnine.experimentalAutoImports": true,
    "python.createEnvironment.trigger": "off",
    "explorer.confirmDragAndDrop": false,

    // Configurações do TODO Tree
    "todo-tree.highlights.customHighlight": {
       "TODO": {
      "icon": "checkbox",
      "foreground": "#000000", 
      "background": "#FFFF00",
      "iconColour": "#FFFF00"
    },
    "NOTE": {
      "icon": "note",
      "foreground": "#FFFFFF",
      "background": "#6495ED",
      "iconColour": "#6495ED"
    },
    "USEFUL": {
      "icon": "verified",
      "foreground": "#000000",
      "background": "#66CDAA",
      "iconColour": "#66CDAA"
    },
    "COMMENT": {
      "icon": "comment",
      "foreground": "#FFFFFF",
      "background": "#808080",
      "iconColour": "#FFFFFF"
    },
    "LEARN": {
      "icon": "bookmark",
      "foreground": "#FFFFFF",
      "background": "#FF69B4",
      "iconColour": "#FF69B4"
    },
    "FIXME": {
      "icon": "tools",
      "foreground": "#DC143C",
      "background": "#DEB887",
      "iconColour": "#DEB887"
    },
    "RECHECK": {
      "icon": "codescan",
      "foreground": "#FFFFFF",
      "background": "#D2691E",
      "iconColour": "#D2691E"
    },
    "INCOMPLETE": {
      "icon": "alert",
      "foreground": "#FFFFFF",
      "background": "#C71585",
      "iconColour": "#C71585"
    },
    "BUG": {
      "icon": "bug",
      "foreground": "#FFFFFF",
      "background": "#DC143C",
      "iconColour": "#DC143C"
    },
    "SEE NOTES": {
      "icon": "note",
      "foreground": "#FFFFFF",
      "background": "#008080",
      "iconColour": "#008080"
    },
    "POST": {
      "icon": "share",
      "foreground": "#FFFFFF",
      "background": "#008000",
      "iconColour": "#008000"
    },
    "[ ]": {
      "icon": "check",
      "foreground": "#000000",
      "background": "#FFFFFF",
      "iconColour": "#FFFF00"
    },
    "[x]": {
      "icon": "check",
      "foreground": "#FFFFFF",
      "background": "#008000",
      "iconColour": "#008000"
    }
  },
  "todo-tree.general.tags": [
    "BUG",
    "SEE NOTES",
    "HACK",
    "FIXME",
    "RECHECK",
    "INCOMPLETE",
    "TODO",
    "NOTE",
    "POST",
    "USEFUL",
    "LEARN",
    "COMMENT",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS).*(\\n\\s*//\\s{2,}.*)*"
}
